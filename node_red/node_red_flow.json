    {
        "id": "8b8926d6aa77be6e",
        "type": "function",
        "z": "43a1d45bd1f2ab4b",
        "name": "Compute Fleet KPIs",
        "func": "const arr = msg.payload || [];\nconst total = arr.length;\nconst overspeed = arr.filter(v => v.status === \"OVERSPEED\").length;\nconst latest = arr.length ? arr.map(v => v.timestamp).sort().slice(-1)[0] : \"N/A\";\n\nmsg.payload = { total, overspeed, latest };\nreturn msg;",
        "outputs": 1,
        "timeout": 0,

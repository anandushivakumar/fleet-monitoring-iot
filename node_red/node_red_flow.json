        "name": "Fleet Map",
        "order": 1,
        "width": 0,
        "height": 0,
        "head": "",
        "format": "<template>\n  <div id=\"fleetMap\" style=\"height: 550px; width: 100%; border-radius: 12px;\"></div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      map: null,\n      markers: {}\n    };\n  },\n\n  mounted() {\n    // Load Leaflet dynamically once the component mounts\n    const css = document.createElement(\"link\");\n    css.rel = \"stylesheet\";\n    css.href = \"https://unpkg.com/leaflet@1.9.4/dist/leaflet.css\";\n    document.head.appendChild(css);\n\n    const script = document.createElement(\"script\");\n    script.src = \"https://unpkg.com/leaflet@1.9.4/dist/leaflet.js\";\n    script.onload = () => {\n      this.initMap();\n      // In case first msg already arrived before leaflet loaded\n      this.updateMarkersFromMsg();\n    };\n    document.head.appendChild(script);\n  },\n\n  watch: {\n    msg: {\n      handler() {\n        this.updateMarkersFromMsg();\n      },\n      deep: true\n    }\n  },\n\n  methods: {\n    initMap() {\n      if (this.map || !window.L) return;\n\n      this.map = window.L.map(\"fleetMap\").setView([25.276987, 55.296249], 11);\n\n      window.L.tileLayer(\"https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png\", {\n        maxZoom: 19,\n        attribution: \"&copy; OpenStreetMap\"\n      }).addTo(this.map);\n\n      // Fix blank map due to hidden render/layout\n      setTimeout(() => this.map.invalidateSize(), 300);\n    },\n\n    upsertMarker(v) {\n      if (!this.map || !window.L) return;\n\n      const id = v.vehicle_id;\n      const lat = Number(v.latitude);\n      const lon = Number(v.longitude);\n\n      if (Number.isNaN(lat) || Number.isNaN(lon)) return;\n\n      const popup = `\n        <b>${id}</b><br/>\n        Speed: ${v.speed} km/h<br/>\n        Status: ${v.status}<br/>\n        Time: ${v.timestamp}\n      `;\n\n      if (!this.markers[id]) {\n        this.markers[id] = window.L.marker([lat, lon])\n          .addTo(this.map)\n          .bindPopup(popup);\n      } else {\n        this.markers[id].setLatLng([lat, lon]).setPopupContent(popup);\n      }\n    },\n\n    updateMarkersFromMsg() {\n      // msg is provided by Dashboard 2 automatically\n      const vehicles = (this.msg && this.msg.payload) ? this.msg.payload : [];\n\n      // Ensure map is initialized once Leaflet is ready\n      this.initMap();\n\n      // Update all markers\n      vehicles.forEach(v => this.upsertMarker(v));\n    }\n  }\n};\n</script>",
